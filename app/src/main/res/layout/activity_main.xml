<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:mapbox="http://schemas.android.com/apk/res-auto"
        tools:context="com.example.apparcado.MainActivity">

    <com.mapbox.mapboxsdk.maps.MapView
            android:id="@+id/mapView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            mapbox:mapbox_cameraTargetLat = "40.4167754"
            mapbox:mapbox_cameraTargetLng = "-3.7037902"
            mapbox:mapbox_cameraZoom = "30"
    />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/nav"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_margin="@dimen/fab_margin"
            android:src="@drawable/ic_navigation2"
            app:backgroundTint="@color/mapboWhite"/>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/park"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_margin="@dimen/fab_margin"
            android:src="@drawable/ic_park"
            app:backgroundTint= "@color/mapboWhite"/>

</androidx.constraintlayout.widget.ConstraintLayout>